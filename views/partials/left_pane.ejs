<div id="left">
    <div class="left_container">
        <ul id="entry_list">
            <li class="entry_control">
                <div class="entry_item entry_control">
                    <div>
                        <a href="/">
                            New Entry
                        </a>
                    </div>
                </div>
            </li>
            <li class="entry_control">
                <div class="entry_item entry_control">
                    <div>
                        <a href="/entry/random">
                            Random Entry
                        </a>
                    </div>
                </div>
            </li>
        </ul>
        <div class="top_left">
            <span class="top_left_text">
                Current Entry
            </span>
        </div>
        <ul id="current_entry"></ul>
        <div class="top_left">
            <span class="top_left_text">
                Recent Entries
            </span>
        </div>
        <ul id="entry_list">
            <% for( let entry of entries) { %>
                <li>
                    <div class="entry_item" id="<%= entry._id %>">
                        <div>
                            <a href="/entry/<%= entry._id %> "><%= entry.title %></a>
                        </div>
                        <div id="date_label">
                            <%= new Intl.DateTimeFormat('en-US', { weekday: 'long', year: 'numeric', month: 'long', day: 'numeric' }).format(entry.date) %> 
                        </div>
                    </div>
                </li>
            <% } %>
        </ul>
    </div>
</div>